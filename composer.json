{
    "name": "ticketpark/file-bundle",
    "type": "symfony-bundle",
    "description": "This bundle assists in caching and downloading local and remote files",
    "keywords": ["file", "filesystem"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ticketpark contributors and others",
            "homepage": "https://github.com/Ticketpark/TicketparkFileBundle/contributors"
        }
    ],

    "require": {
        "php":                ">=5.3.2",
        "sensio/buzz-bundle": "dev-master"
    },

    "require-dev": {
        "twig/twig": ">=1.0,<2.0-dev"
    },

    "suggest": {
        "twig/twig": ">=1.0,<2.0-dev"
    },

    "autoload": {
        "psr-0": { "Ticketpark\\FileBundle": "" }
    },

    "target-dir": "Ticketpark/FileBundle"
}
